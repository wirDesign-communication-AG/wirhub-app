{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": false,
        "preferred-install": {
            "wirdesign-communication-ag/wirhub": "source"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "endpoint": [
                "https://api.github.com/repos/wirdesign-communication-ag/wirhub-recipes/contents/index.json",
                "flex://defaults"
            ],
            "require": "^7.2"
        }
    },
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "repositories": [
        {
            "package": {
                "name": "wirdesign-communication-ag/wirhub"
            },
            "type": "github",
            "url": "https://github.com/wirDesign-communication-AG/wirhub"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-imagick": "^3.4.3",
        "ext-mbstring": "*",
        "doctrine/doctrine-bundle": "^2.9",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^3",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "^7.2",
        "symfony/dotenv": "^7.2",
        "symfony/expression-language": "^7.2",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "^7.2",
        "symfony/runtime": "^7.2",
        "symfony/yaml": "^7.2",
        "wirdesign-communication-ag/wirhub": "wip/1076-dev"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "type": "project",
    "require-dev": {
        "ext-sqlite3": "*",
        "dg/bypass-finals": "^1.4",
        "doctrine/doctrine-fixtures-bundle": "^4.0.0",
        "friendsofphp/php-cs-fixer": "^3.59",
        "liip/test-fixtures-bundle": "^3.2.1",
        "phpstan/phpstan": "^2.1.0",
        "phpunit/phpunit": "^11.5.2",
        "rector/rector": "^2.0.5",
        "symfony/browser-kit": "^7.2",
        "symfony/css-selector": "^7.2",
        "symfony/debug-bundle": "^7.2",
        "symfony/maker-bundle": "^1.48",
        "symfony/panther": "^2.0",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "^7.2",
        "symfony/var-exporter": "^7.2",
        "symfony/web-profiler-bundle": "^7.2"
    }
}
